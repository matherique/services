{
  "extends": [
    "wesbos"
  ],
  "rules": {
    "no-unused-vars": 0,
    "class-methods-use-this": 0,
    "indent": [
      "error",
      2
    ],
    "import/named": 0,
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "parser": "flow",
        "trailingComma": "es5",
        "tabWidth": 2
      }
    ]
  }
}